op) => {\r\n      if (uid) {\r\n        console.log(snapshop.val()[uid]);\r\n        get(snapshop.val()[uid].userName);\r\n      }\r\n    });\r\n  }\r\n}\r\n","C:\\Users\\ì—¼ì¬ì„ \\Desktop\\studies\\todolist_react\\src\\components\\Ranking\\Ranking.tsx",["159","160","161","162"],"import React, { useEffect, useState } from 'react';\r\nimport { DatabaseType } from 'src/services/data_service';\r\nimport * as S from './Ranking.style';\r\n\r\ntype PropType = {\r\n  databaseService: DatabaseType;\r\n};\r\n\r\ntype StateType = {\r\n  time: number;\r\n  userName: string;\r\n}[];\r\n\r\nconst hour = (time: number): number => Math.floor(time / 3600);\r\nconst minute = (time: number): number => Math.floor(time / 60) % 60;\r\n\r\nconst Ranking: React.FC<PropType> = ({ databaseService }) => {\r\n  const [rankers, setRankers] = useState<StateType>([]);\r\n\r\n  useEffect(() => {\r\n    const datasSync = databaseService.getUserDatas((datas) => {\r\n      if (datas) {\r\n        const infos = Object.keys(datas)\r\n          .map((key) => ({\r\n            time: +datas[key].time,\r\n            userName: datas[key].userName,\r\n          }))\r\n          .sort((a, b) => {\r\n            if (a.time > b.time) return -1;\r\n            if (a.time < b.time) return 1;\r\n            else return 0;\r\n          });\r\n        setRankers(infos);\r\n      } else {\r\n        setRankers([]);\r\n      }\r\n    });\r\n\r\n    return () => datasSync();\r\n  }, []);\r\n\r\n  return (\r\n    <S.RankingContainer>\r\n      <S.RankingTitle>ë­í‚¹</S.RankingTitle>\r\n      <S.UserList>\r\n        {rankers.map((user, idx) => {\r\n          return (\r\n            <S.User rank={idx} key={user.userName}>\r\n              <S.rankNum>{idx + 1}ë“±</S.rankNum>\r\n              <S.rankName>\r\n                {idx <= 2 ? <S.CrwonIcon /> : <S.NoRankerUser></S.NoRankerUser>}\r\n                {user.userName}\r\n              </S.rankName>\r\n              <S.rankTIme>{`${hour(user.time)}ì‹œê°„ ${minute(\r\n   